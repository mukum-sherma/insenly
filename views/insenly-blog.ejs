<script>
        var el = document.activeElement;

        var ref = firebase.database().ref('Blog1');
        ref.on("value", function(snapshot) {
          //working --> console.log(snapshot.val().Comment.Comment1);

         //document.getElementById("addComments").innerHTML="";
        //document.getElementById("postImg").innerHTML="";
        document.getElementById("commentLists").innerHTML = "";

         // convert into json 
         // {Comment1:"Hi","Comment2":"Hello"}
          var myCom = JSON.stringify(snapshot.val());

          //coverts into javascript object or dictionary like
          var myComObjects = JSON.parse(myCom);
          
          $("#showHideCommentBtn").text("Loading ...");
          document.getElementById("showHideCommentBtn").disabled = true;

          if (myComObjects == null) {
            $("#showHideCommentBtn").text("No Comment Yet");
          }

          console.log(snapshot.val());

          // indexed iteration
          console.log(Object.keys(myComObjects));
          var myComObjectsKeys = Object.keys(myComObjects);
          //console.log(myComObjects[myComObjectsKeys[0]]);
        
          //const showBtnValue = $("#showHideCommentBtn").text();
          //console.log("+++ 00 " +showBtnValue);

          $("#showHideCommentBtn").text("YYYYY");
          for (var i=0; i < myComObjectsKeys.length; i++) {
            var myComObject = myComObjects[myComObjectsKeys[i]];

            var divImg = document.createElement("div");
            var divCommentHeading = document.createElement("div");
            divCommentHeading.setAttribute("class","comment-heading");
            var divCommentBody = document.createElement("div");
            divCommentBody.setAttribute("class","comment-body")
            divCommentBody.setAttribute("id","mainCommentBody");

            var a = document.createElement("a");
            a.setAttribute("class","pull-left");
            a.setAttribute("href","#");
            var img = document.createElement("img");
            img.setAttribute("class","avatar");
            // img.setAttribute("src","http://bootdey.com/img/Content/user_3.jpg");
            img.setAttribute("src","https://en.opensuse.org/images/0/0b/Icon-user.png");
            img.setAttribute("alt","avatar");
            // var img = document.createElement("img");
            // img.src = "img/userIcon.png";
            // // img.setAttribute("Content","img/insenly_02.png");
            img.setAttribute("width","60px");
            img.setAttribute("height","60px");
            // img.setAttribute("margin-right","60px");
            a.appendChild(img)

            var h4 = document.createElement("h4");
            h4.setAttribute("class","user");
            h4.setAttribute("position","absolute");
            h4.setAttribute("margin-right","200px");
            var ht4 = document.createTextNode(myComObject.name);
            h4.appendChild(ht4);

            var h5 = document.createElement("h6");
            h5.setAttribute("class","time");
            h5.setAttribute("font-size","10");
            var ht5 = document.createTextNode(myComObject.postDate);
            //h5.setAttribute("margin-left","200px");
            h5.appendChild(ht5);


            var p = document.createElement("p");
            var t = document.createTextNode(myComObject.comment);
            p.setAttribute("padding-left","600px")
            p.appendChild(t);
            var divMainComment = document.createElement("div")
            divMainComment.setAttribute("id","mainComment");
            divMainComment.appendChild(p);

            divImg.appendChild(a);
            // divCommentHeading.appendChild(divCommentBody);
            // divCommentBody.appendChild(h4);
            // divCommentBody.appendChild(h5);
            // divCommentBody.appendChild(divMainComment);
            // divCommentBody.setAttribute("padding-left","20px");

            // document.getElementById("commentLists").appendChild(divImg);
            // document.getElementById("commentLists").appendChild(divCommentHeading);

            document.getElementById("commentUserIcon").appendChild(divImg);
            document.getElementById("commentUserName").appendChild(h4);
            document.getElementById("commentTime").appendChild(h5);
            document.getElementById("commentData").appendChild(p);
             
             //check if it's the last item in the object
            $("#showHideCommentBtn").text("Loading...");

            if (i == (myComObjectsKeys.length - 1)) {
                 //var showBtn = document.getElementById('showHideCommentBtn');
                // $("#showHideCommentBtn").removeAttr('disabled');
                 $("#showHideCommentBtn").text("Show Comments");
                 
            }
          }

       },
       function (error) {
          console.log("Error: " + error.code);
       });
</script>
